{
    "version": 2,
    "builds": [{ "src": "index.js", "use": "@now/node-server" }],
    "routes": [
        {
          "src": "/api",
          "dest": "/index.js",
          "methods": ["GET"]
        },
        {
          "src": "/api/video/youtube",
          "dest": "/index.js",
          "methods": ["POST"]
        },
        {
          "src": "/api/video/youtube/info",
          "dest": "/index.js",
          "methods": ["POST"]
        }
      ]
  }